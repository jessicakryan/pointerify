[
    {
        "el": "root",
        "on": "mousedown",
        "bind": "handleMouseDown"
    },
    {
        "el": "root",
        "on": "touchstart",
        "bind": "handleTouchStart",
        "passive": false
    },
    {
        "el": "root",
        "on": "click",
        "bind": "handleClick"
    },
    {
        "el": "root",
        "on": "mousemove",
        "bind": "handleRootMouseMove"
    },
    {
        "on": "mousemove",
        "bind": "handleWindowMouseMove"
    },
    {
        "on": "touchmove",
        "bind": "handleTouchMove",
        "passive": false
    },
    {
        "on": "mouseup",
        "bind": "handleMouseUp"
    },
    {
        "on": "touchend",
        "bind": "handleTouchEnd",
        "passive": false
    },
    {
        "on": "resize",
        "bind": "handleResize",
        "debounce": 100
    }
]